<MudDialog>
    <DialogContent>
        @foreach (var gameInformation in _pluginManager.GetAvailablePluginInformation())
        {
            <GameCard GameInformation=@gameInformation/>
        }
    </DialogContent>
    <DialogActions>
        <MudButton OnClick="Cancel">Cancel</MudButton>
    </DialogActions>
</MudDialog>

@code {
    [CascadingParameter] MudDialogInstance MudDialog { get; set; }

    void Cancel() => MudDialog.Cancel();
}